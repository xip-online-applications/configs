{
  "name": "browserslist-config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/browserslist-config/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/browserslist-config",
        "main": "packages/browserslist-config/index.js",
        "tsConfig": "packages/browserslist-config/tsconfig.lib.json",
        "assets": ["packages/browserslist-config/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs browserslist-config {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/browserslist-config/**/*.js"]
      }
    }
  },
  "tags": []
}
